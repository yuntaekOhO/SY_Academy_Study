<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>실습</title>
<script type="text/javascript">
/*
 다섯개의 숫자를 입력하고 입력한 숫자를 배열에 저장한 후
 배열의 요소 중 최대값과 최소값을 출력하시오
 */
 
	let array = [];
	let no1, no2, no3, no4, no5, min, max;
	no1 = prompt('첫번째 숫자', '');
	no2 = prompt('두번째 숫자', '');
	no3 = prompt('세번째 숫자', '');
	no4 = prompt('네번째 숫자', '');
	no5 = prompt('다섯번째 숫자', '');

	array.push(no1, no2, no3, no4, no5);

	array.sort(function(left, right) {
		return left - right;
	});
	min = array[0];
	max = array[4];

	document.write(array + '<br>');
	document.write('최대값 : ' + max + '<br>');
	document.write('최소값 : ' + min + '<br>');
</script>
</head>
<body>

</body>
</html>